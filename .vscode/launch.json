{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Trace sim - HW mesh 1 and best SNN",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "bin/Debug/net6.0/SpikingDSE.dll",
            "args": ["trace-sim", "-s", "res/exp/snn-best.json", "-h", "res/exp/hw-mesh1.json", "-m", "res/exp/mapping1-best.json", "-t", "res/dataset/shd-10.zip;input_0.trace", "-o", "res/results/test"],
            "cwd": "${workspaceFolder}/Processor",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "Mapping - Exp 1 hw and ssc2",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "bin/Debug/net6.0/SpikingDSE.dll",
            "args": ["mapping", "-s", "res/snn/snn-ssc2.json", "-h", "res/exp/exp1/hw.json", "-m", "FirstFit1", "-o", "res/exp/mappings/ssc2.json"],
            "cwd": "${workspaceFolder}/Processor",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
    ]
}